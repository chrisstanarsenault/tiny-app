<!DOCTYPE html>
<html lang="en">

<head>
  <title>TinyApp</title>
</head>

<body>
  <!-- Add your EJS code here -->
  <header>
    <% include ./partials/_header %>
  </header>

  <ul>
      <% for (let eachUrl in urls) { %>
        <li>
          <form method="POST" action="/urls/<%= eachUrl %>/delete">
            <%= eachUrl %> :
            <a href="<%= urls[eachUrl] %>"><%= urls[eachUrl] %></a>
            <a href="/urls/<%= eachUrl %>">edit</a>
            <input type="submit" value="Delete Me" style="margin-left: 10px">
          </form>
        </li>
      <% }; %>

    </ul>

</body>

</html>